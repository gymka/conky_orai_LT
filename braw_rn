<script>
	(function (jQuery) {
		var pco = TWC.pco;
		var testPcoData = {}; 
	    jQuery(document).ready(function() {
	    	jQuery('.wx-obs-pco-data').each( function(index, Element) {
	    		var pcoMod = jQuery(Element);
	    		var locId = pcoMod.attr("data-location-id");
	    		var locType = pcoMod.attr("data-location-type");
	    		var locPresName = pcoMod.attr("data-location-presentation-name"); 
	    		var pcoCCDataKey = [locId,":",locType].join('');
	    		var pcoCCData = {};
	    			pcoCCData['location-presentation-name'] = locPresName;
	    			testPcoData['location-presentation-name'] = locPresName;
		    		pcoMod.find(".wx-value").filter("[itemprop]").each( function(index, Element) {
		    			var pcoItem = jQuery(Element);
		    			pcoCCData[pcoItem.attr('itemprop')] = pcoItem.html();
			    		testPcoData[pcoItem.attr('itemprop')] = pcoItem.html();
		    		});
	    	});
	    });
		jQuery(document).ready(function() {
			jQuery('.wx-current').each( function(index, Element) {
				var ccMod = jQuery(Element);
	    		var locId = ccMod.attr("data-location-id");
	    		var locType = ccMod.attr("data-location-type");
	    		var pcoCCDataKey = [locId,":",locType].join('');
		    		var appId = 144457238950063;
				   	var name =  encodeURIComponent("The Weather Channel");
				   	var description = encodeURIComponent("The Weather is " + testPcoData['weather-phrase'] + " and Temperature is " + testPcoData['g-celsius']);
				   	var caption =  encodeURIComponent("The Current Conditions for " + testPcoData['location-presentation-name']);
				   	var pictureUrl = encodeURIComponent(testPcoData['weather-icon']);
				   	var link = encodeURIComponent("http://www.weather.com/weather/today/" + locId);
				   	var redirectUrl =  encodeURIComponent("http://www.weather.com/");
				    var fbUrl = "https://www.facebook.com/dialog/feed?app_id="+appId+"&name="+name+"&description="+description+"&caption="+caption+"&picture="+pictureUrl+"&link="+link+"&redirect_uri="+redirectUrl;
		    		ccMod.find(".wx-fb-share").first().attr('href',fbUrl);
	    	});
		});
	})(jQuery);        
</script>
