		<div id="wx-bottom-wrap">
		<div id="wx-bottom-bg">
		<div id="wx-bottom-inner">
<script src="http://tags.crwdcntrl.net/c/1884/cc.js?ns=_cc1884" id="LOTCC_1884"></script>
<script>
 // The below code is put in by HCL folks for lotame settings in pco. This should ideally get called before page load completes and ad cust_params construction happens after that.
 // Modified By: Kulanthaivelu Sankaran
 // Modified: Removed onDocumentReady dependency to kickoff this operation asap before ad calls kicks off and added comma support for Audience array.
(function($){
	$(document).ready(function() {
		var url1 = 'http://ad.crwdcntrl.net/5/c=1884/pe=y/callback=?';
		$.ajax({
			url: url1,
			dataType: 'jsonp',
			cache: true,	  
			type:'GET',
			timeout: 5000,
			success: function(data) {	 
				if (TWC.pco.get('user.lotame')==null || typeof (TWC.pco.get('user.lotame'))=="undefined") { 
					TWC.pco.setUser('lotame', {});
				}
				var lot_tpid, lot_abbr, lot_id, lotame;		
				lotame = TWC.pco.get('user.lotame');	
				lot_tpid = lotame.lot_tpid;
				lot_abbr = lotame.lot_abbr;
				lot_id = lotame.lot_id;
				lotame.lot_tpid = data.Profile.tpid;
				var audienceLength = data.Profile.Audiences.Audience.length;
				if(audienceLength > 0) {
					lot_abbr = '';
					lot_id = '';
					for (var cci = 0; cci < audienceLength; cci++)	{			
					   lot_abbr += (data.Profile.Audiences.Audience[cci].abbr + (cci < audienceLength-1 ? ',' : ''));
					   lot_id +=  (data.Profile.Audiences.Audience[cci].id + (cci < audienceLength-1 ? ',' : ''));
					}
					lotame.lot_abbr = lot_abbr;
					lotame.lot_id = lot_id;
				}
				TWC.pco.setUser('lotame', lotame);
				if(_cc1884) {
					if(TWC.pco.get("user")!=null && typeof(TWC.pco.get("user"))!="undefined"){ 
						_cc1884.add("age",TWC.pco.get("user").age); 
						_cc1884.add("gender",TWC.pco.get("user").gender); 
						_cc1884.add("seg", "claritas_"+TWC.pco.get("user").claritas); 
						_cc1884.add("seg","preferredDeclaration_"+TWC.pco.get("user").preferredDeclaration); 
						if(TWC.pco.get("user.hardDeclarations")!=null && typeof(TWC.pco.get("user.hardDeclarations"))!="undefined"){ 
							if(TWC.pco.get("user").hardDeclarations.length > 0){
							    _cc1884.add("seg","hardDeclarations_"+TWC.pco.get("user").hardDeclarations[0]); 
							}
						} 
						if(TWC.pco.get("user.softDeclarations")!=null && typeof(TWC.pco.get("user.softDeclarations"))!="undefined"){ 
							if(TWC.pco.get("user").softDeclarations.length > 0){
						   		_cc1884.add("seg","softDeclarations_"+TWC.pco.get("user").softDeclarations[0]); 
						   	}
						}	
						_cc1884.add("seg","preferredLocation_locid_"+TWC.pco.get("user").preferredLocation.locid); 
						_cc1884.add("seg","preferredLocation_dma_"+TWC.pco.get("user").preferredLocation.dma); 
						if(TWC.pco.get("user.recentSearchLocations")!=null && typeof(TWC.pco.get("user.recentSearchLocations"))!="undefined"){ 
							if(TWC.pco.get("user").recentSearchLocations.length > 0){
				               _cc1884.add("seg","recentSearchLocations_locid_"+TWC.pco.get("user").recentSearchLocations[0].locid); 
							_cc1884.add("seg","recentSearchLocations_dma_"+TWC.pco.get("user").recentSearchLocations[0].dma); 
							}
						}	
					}		 
					if(TWC.pco.get("currloc")!=null && typeof(TWC.pco.get("currloc"))!="undefined"){ 
						_cc1884.add("seg","zip_"+TWC.pco.get("currloc").zip); 
						_cc1884.add("seg","lon_"+TWC.pco.get("currloc").lon); 
						_cc1884.add("seg","locid_"+TWC.pco.get("currloc").locid); 
						_cc1884.add("seg","dma_"+TWC.pco.get("currloc").dma); 
						_cc1884.add("seg","claritas_"+TWC.pco.get("currloc").claritas); 
						_cc1884.add("seg","state_"+TWC.pco.get("currloc").state); 
						_cc1884.add("seg","locTimeZoneAbbr_"+TWC.pco.get("currloc").locTimeZoneAbbr); 
						_cc1884.add("seg","city_"+TWC.pco.get("currloc").city); 
						_cc1884.add("seg","country_"+TWC.pco.get("currloc").country); 
						_cc1884.add("seg","locdate_"+TWC.pco.get("currloc").locdate); 
						_cc1884.add("seg","loctype_"+TWC.pco.get("currloc").loctype); 
						_cc1884.add("seg","locname_"+TWC.pco.get("currloc").locname); 
						if(TWC.pco.get("currloc.gpr")!=null && typeof(TWC.pco.get("currloc.gpr"))!="undefined"){ 
							if(TWC.pco.get("currloc").gpr.length > 0){
								_cc1884.add("seg","gpr_"+TWC.pco.get("currloc").gpr[0]); 
							}
						} 
						_cc1884.add("seg","lat_"+TWC.pco.get("currloc").lat); 
						}	
						if(TWC.pco.get("page")!=null && typeof(TWC.pco.get("page"))!="undefined"){ 
							_cc1884.add("seg","loctime_",TWC.pco.get("page").loctime); 
							_cc1884.add("seg","pageId_"+TWC.pco.get("page").pageId); 
							_cc1884.add("seg","locale_"+TWC.pco.get("page").locale); 
							_cc1884.add("seg","lang_"+TWC.pco.get("page").lang); 
							_cc1884.add("seg","ampm_"+TWC.pco.get("page").ampm); 
						} 
						if(TWC.pco.get("ad")!=null && typeof(TWC.pco.get("ad"))!="undefined"){ 			
								if(TWC.pco.get("ad.site")!=null && typeof(TWC.pco.get("ad.site"))!="undefined"){ 
								_cc1884.add("seg","site_"+TWC.pco.get("ad").site); 
						} 
						if(TWC.pco.get("ad.pageIdCode")!=null && typeof(TWC.pco.get("ad.pageIdCode"))!="undefined"){ 
								_cc1884.add("seg","pageIdCode_"+TWC.pco.get("ad").pageIdCode); 
						} 
						if(TWC.pco.get("ad.mode")!=null && typeof(TWC.pco.get("ad.mode"))!="undefined"){ 
								_cc1884.add("seg","mode_"+TWC.pco.get("ad").mode); 
						} 
						if(TWC.pco.get("ad.pageDeclaration")!=null && typeof(TWC.pco.get("ad.pageDeclaration"))!="undefined"){ 
								_cc1884.add("seg","pageDeclaration_"+TWC.pco.get("ad").pageDeclaration); 
						} 
						if(TWC.pco.get("ad.zone")!=null && typeof(TWC.pco.get("ad.zone"))!="undefined"){ 
								_cc1884.add("seg","zone_"+TWC.pco.get("ad").zone); 
						} 
					} 
					if(TWC.pco.get("wx")!=null && typeof(TWC.pco.get("wx"))!="undefined"){ 
						_cc1884.add("seg","wind_"+TWC.pco.get("wx").wind); 
						_cc1884.add("seg","hum_"+TWC.pco.get("wx").hum); 
						_cc1884.add("seg","relativeHumidity_"+TWC.pco.get("wx").relativeHumidity); 
						_cc1884.add("seg","cwsh_"+TWC.pco.get("wx").fcast.cwsh); 
						_cc1884.add("seg","cwsl_"+TWC.pco.get("wx").fcast.cwsl); 
						_cc1884.add("seg","fsnw_"+TWC.pco.get("wx").fcast.fsnw); 
						_cc1884.add("seg","tempH_"+TWC.pco.get("wx").fcast.tempH); 
						_cc1884.add("seg","tempHR_"+TWC.pco.get("wx").fcast.tempHR); 
						_cc1884.add("seg","tempL_"+TWC.pco.get("wx").fcast.tempL); 
						_cc1884.add("seg","tempLR_"+TWC.pco.get("wx").fcast.tempLR); 
						_cc1884.add("seg","fc1_"+TWC.pco.get("wx").fcast.cond.fc1); 
						_cc1884.add("seg","fc2_"+TWC.pco.get("wx").fcast.cond.fc2); 
						_cc1884.add("seg","fc3_"+TWC.pco.get("wx").fcast.cond.fc3); 
						_cc1884.add("seg","cond_"+TWC.pco.get("wx").cond); 
						_cc1884.add("seg","snw_"+TWC.pco.get("wx").snw); 
						_cc1884.add("seg","tempR_"+TWC.pco.get("wx").tempR); 
						_cc1884.add("seg","windSpeed_"+TWC.pco.get("wx").windSpeed); 
						_cc1884.add("seg","pollen_"+TWC.pco.get("wx").pollen); 
						_cc1884.add("seg","baroTendency_"+TWC.pco.get("wx").baroTendency); 
						_cc1884.add("seg","uv_"+TWC.pco.get("wx").uv); 
						_cc1884.add("seg","uvIndex_"+TWC.pco.get("wx").uvIndex); 
						_cc1884.add("seg","temp_"+TWC.pco.get("wx").temp); 
						if(TWC.pco.get("wx.severe")!=null && typeof(TWC.pco.get("wx.severe"))!="undefined"){ 
							if(TWC.pco.get("wx").severe.length > 0){
							_cc1884.add("seg","severe_"+TWC.pco.get("wx").severe[0]); 
							}
						} 
						_cc1884.add("seg","realTemp_"+TWC.pco.get("wx").realTemp);	
					}	
					_cc1884.bcp();
				}
		  }, error: function (jqXHR, textStatus, errorThrown) {
		  }
		});
	});
